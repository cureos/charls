        <file src="cscharls_portable\bin\Release\cscharls.dll" target="lib\portable-net45+win8+wpa81\cscharls.dll" />
        <file src="cscharls_portable\bin\Release\cscharls.pdb" target="lib\portable-net45+win8+wpa81\cscharls.pdb" />
